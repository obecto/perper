{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "customHandler": {
    "description": {
      "defaultExecutablePath": "jupyter-notebook",
      "arguments": ["--port",
        "%FUNCTIONS_CUSTOMHANDLER_PORT%",
        "--NotebookApp.nbserver_extensions",
        "{'perper.custom_handler.custom_handler':True}",
        "--NotebookApp.default_url",
        "/lab" 
      ]
    }
  }
}
